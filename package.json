  "devDependencies": {
    "@types/jszip": "^3.1.6",
    "@types/node": "^12.6.2",
    "@types/pug": "^2.0.4",
    "@types/shelljs": "^0.8.5",
    "@wwawing/assets": "0.0.0-test.5",
    "@wwawing/debug-server": "0.0.0-test.5",
